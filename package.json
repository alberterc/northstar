{
  "name": "northstar",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run build:frontend && wrangler dev ./backend/src/index.js",
    "deploy": "npm run build:frontend && wrangler deploy ./backend/src/index.js",
    "dev:frontend": "vite --config ./frontend/vite.config.js frontend",
    "build:frontend": "vite build --config ./frontend/vite.config.js frontend",
    "preview:frontend": "vite preview --config ./frontend/vite.config.js frontend"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@tailwindcss/vite": "^4.1.11",
    "eslint": "^9.32.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.11",
    "vite": "^7.0.4",
    "wrangler": "^4.28.0"
  }
}
